{
  "expo": {
    "name": "SahiMap",
    "slug": "SahiMap",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey":"Your API Key"
      }
    },
    "android": {
      "package":"com.sahimap.location",
      "config": {
        "googleMaps": {
          "apiKey":"Your API key"
        }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
